# Increase volume
"$HOME/scripts/audio/volup.sh"
   XF86AudioRaiseVolume

# Decrease volume
"$HOME/scripts/audio/voldown.sh"
   XF86AudioLowerVolume

# Mute volume
"$HOME/scripts/audio/mutetoggle.sh"
   XF86AudioMute

# Set Output Headphones
"$HOME/scripts/audio/toheadphones.sh"
   XF86AudioPrev
# Set Output Speakers
"$HOME/scripts/audio/tospeakers.sh"
   XF86AudioNext
