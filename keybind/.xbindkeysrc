# Increase volume
"pactl set-sink-mute combined false ; pactl set-sink-volume combined +1%"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-mute combined false ; pactl set-sink-volume combined -1%"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute combined toggle"
   XF86AudioMute

# Set Output Headphones
"pactl set-sink-mute 1 false ; pactl set-sink-mute 0 true"
   XF86AudioPrev
# Set Output Speakers
"pactl set-sink-mute 0 false ; pactl set-sink-mute 1 true"
   XF86AudioNext
