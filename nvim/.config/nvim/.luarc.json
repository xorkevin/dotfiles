{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "?.lua",
      "?/init.lua"
    ],
    "pathStrict": true
  },
  "workspace": {
    "library": [
      "/usr/share/nvim/runtime/lua",
      "~/.config/nvim/nvimluatypes",
      "~/.local/share/nvim/site/pack/packer/start/LuaSnip/lua",
      "~/.local/share/nvim/site/pack/packer/start/cmp-buffer/lua",
      "~/.local/share/nvim/site/pack/packer/start/cmp-nvim-lsp/lua",
      "~/.local/share/nvim/site/pack/packer/start/cmp-path/lua",
      "~/.local/share/nvim/site/pack/packer/start/cmp_luasnip/lua",
      "~/.local/share/nvim/site/pack/packer/start/fzf-lua/lua",
      "~/.local/share/nvim/site/pack/packer/start/gitsigns.nvim/lua",
      "~/.local/share/nvim/site/pack/packer/start/lsp_signature.nvim/lua",
      "~/.local/share/nvim/site/pack/packer/start/lualine.nvim/lua",
      "~/.local/share/nvim/site/pack/packer/start/mini.align/lua",
      "~/.local/share/nvim/site/pack/packer/start/mini.surround/lua",
      "~/.local/share/nvim/site/pack/packer/start/nvim-base16/lua",
      "~/.local/share/nvim/site/pack/packer/start/nvim-cmp/lua",
      "~/.local/share/nvim/site/pack/packer/start/nvim-lspconfig/lua",
      "~/.local/share/nvim/site/pack/packer/start/nvim-treesitter/lua",
      "~/.local/share/nvim/site/pack/packer/start/nvim-web-devicons/lua",
      "~/.local/share/nvim/site/pack/packer/start/packer.nvim/lua",
      "~/.local/share/nvim/site/pack/packer/start/vim-fugitive/lua",
      "~/.local/share/nvim/site/pack/packer/start/vim-vinegar/lua",
      "~/.local/share/nvim/site/pack/packer/start/vim-visual-star-search/lua"
    ],
    "ignoreDir": [
      "nvimluatypes"
    ],
    "checkThirdParty": false
  }
}
